@model MvcApplication1.Models.Calendario

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Calendario</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.idCalendatio, "Usuario")
        </div>
        <div class="editor-field">
            @Html.DropDownList("idCalendatio", String.Empty)
